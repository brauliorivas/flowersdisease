{
  "dependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "esbuild": "^0.25.9",
    "tailwindcss": "^4.1.11"
  },
  "scripts": {
    "build:css": "tailwindcss -i ./static/src/styles.css -o ./static/dist/styles.css --minify",
    "watch:css": "tailwindcss -i ./static/src/styles.css -o ./static/dist/styles.css --watch",
    "build:js": "esbuild ./static/src/scripts.js --bundle --outfile=./static/dist/scripts.js --minify"
  },
  "devDependencies": {
    "daisyui": "^5.0.50"
  }
}
